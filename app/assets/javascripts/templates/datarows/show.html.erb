<ng-include src="'<%= asset_path('templates/datarows/toolbar.html') %>'"></ng-include>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Date</th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody ng-repeat="point in points | orderBy:'x'">
  	<tr>
      <td><input type="text" ng-change="updatePoint(point)" ng-model="point.x" data-date-format="M dd, yyyy" bs-datepicker readonly class="datepicker"/></td>
      <td><div contenteditable on-change="updatePoint(point)" ng-model="point.y" style="display: inline">{{ point.y }}</div></td>
  	</tr>
  </tbody>
</table>

</div>
